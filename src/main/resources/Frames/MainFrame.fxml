<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="399.0" prefWidth="639.0" xmlns="http://javafx.com/javafx/8.0.161" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox layoutX="105.0" layoutY="23.0" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefWidth="200.0" spacing="10.0">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <children>
                  <Button fx:id="butAdd" mnemonicParsing="false" text="Добавить" />
                  <Button fx:id="butEdit" mnemonicParsing="false" text="Редактировать" />
                  <Button fx:id="butReset" mnemonicParsing="false" text="Сброс всех цен" />
                  <Button fx:id="butSales" mnemonicParsing="false" text="Ввод продаж" />
                  <Button fx:id="butShow" mnemonicParsing="false" text="Показать цены для покупателей" />
               </children>
            </HBox>
            <TableView fx:id="table" VBox.vgrow="ALWAYS">
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy></TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
